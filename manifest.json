{
  "manifest_version": 2,
  "name": "Nemlig.com - Add to basket",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.nemlig.com/*"
      ],
      "js": ["jquery.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
